{
  "name": "@mordech/components",
  "version": "0.24.1",
  "private": true,
  "dependencies": {
    "@mordech/storybook-toggle-theme-addon": "^0.24.1",
    "@mordech/tokens": "^0.24.1",
    "@mordech/web-components": "^0.24.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.58",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-scripts": "5.0.1",
    "typescript": "4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "yarn storybook",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 4200",
    "test": "vitest --coverage --run",
    "test:watch": "vitest",
    "visual": "loki test --reactUri file:./storybook-static",
    "visual:approve": "loki approve",
    "visual:ci": "loki test --requireReference --reactUri file:./storybook-static",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,css}'",
    "generate:component": "nx generate @nrwl/react:component --project=@mordech/components --directory=src/lib/components && nx generate @nrwl/react:component-story --project=@mordech/components"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@nrwl/eslint-plugin-nx": "15.4.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.15",
    "@storybook/api": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/components": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.12",
    "@testing-library/dom": "^8.17.1",
    "@types/jest-axe": "^3.5.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "jest-axe": "^6.0.0",
    "jest-styled-components": "^7.1.1",
    "loki": "^0.30.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react-markdown": "^8.0.3",
    "require-from-string": "^2.0.2",
    "styled-components": "^5.3.5",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "*",
    "vite": "^3.1.6",
    "vitest": "^0.24.1",
    "webpack": "^5.0.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
