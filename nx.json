{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "develop",
          "build",
          "clean",
          "typecheck",
          "storybook",
          "test",
          "lint",
          "visual",
          "eject"
        ],
        "accessToken": "ZWJiZTdjOGEtZmFkYS00YjNhLWIwNTYtOTdkOGRjNTRhMTg4fHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
    "@nrwl/react:component": {
      "style": "styled-components",
      "export": true,
      "pascalCaseDirectory": true,
      "pascalCaseFiles": true
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/build",
        "{projectRoot}/public/build",
        "{projectRoot}/storybook-static"
      ]
    },
    "visual": {
      "dependsOn": ["build"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
