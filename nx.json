{
  "extends": "nx/presets/npm.json",
  "generators": {
    "@nx/react:component": {
      "style": "styled-components",
      "export": true,
      "pascalCaseDirectory": true,
      "pascalCaseFiles": true
    },
    "@nx/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true,
        "skipDefaultProject": true,
        "unitTestRunner": "none"
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint",
        "unitTestRunner": "vitest"
      }
    }
  },
  "targetDefaults": {
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"],
      "cache": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "prepare": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/types",
        "{projectRoot}/build",
        "{projectRoot}/public"
      ],
      "cache": true
    },
    "build-storybook": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/public", "{projectRoot}/storybook-static"],
      "cache": true
    },
    "visual": {
      "dependsOn": ["^build", "build-storybook"],
      "cache": true
    },
    "visual:ci": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "develop": {
      "cache": true
    },
    "build:assets": {
      "cache": true
    },
    "build:css": {
      "cache": true
    },
    "serve": {
      "cache": true
    },
    "test:watch": {
      "cache": true
    },
    "watch:lib": {
      "cache": true
    },
    "clean": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    },
    "storybook": {
      "cache": true
    },
    "lint": {
      "cache": true,
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
    },
    "lint:css": {
      "cache": true
    },
    "@nx/vite:test": {
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"],
      "cache": true
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "defaultProject": "@mordech/portfolio",
  "nxCloudAccessToken": "ZWJiZTdjOGEtZmFkYS00YjNhLWIwNTYtOTdkOGRjNTRhMTg4fHJlYWQtd3JpdGU="
}
